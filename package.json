{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js Boilerplate",
  "author": "keitakn",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server",
    "build": "rm -rf .nuxt dist && tsc -p tsconfig.server.json && nuxt build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/bulma": "^1.2.2",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "nuxt": "^2.4.3",
    "nuxt-property-decorator": "^2.0.1",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express": "^4.16.1",
    "@types/node": "^10.12.24",
    "@types/prettier": "1.16.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "nodemon": "^1.18.10",
    "prettier": "1.16.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
