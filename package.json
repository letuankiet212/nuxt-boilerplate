{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js Boilerplate",
  "author": "keitakn",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "rm -rf .nuxt dist && tsc -p tsconfig.server.json && nuxt build",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --collectCoverage=true",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/bulma": "^1.2.2",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "nuxt": "^2.4.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.6",
    "@types/node": "^10.12.27",
    "@types/prettier": "1.16.1",
    "@types/uuid": "^3.4.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "prettier": "1.16.4",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.3.3",
    "typescript-eslint-parser": "^22.0.0",
    "vue-jest": "^3.0.3"
  }
}
