{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js Boilerplate",
  "author": "keitakn",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "rm -rf .nuxt dist && tsc -p tsconfig.server.json && nuxt build",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --collectCoverage=true",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/bulma": "^1.2.4",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.8.1",
    "ts-node": "^8.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/eslint": "^4.16.7",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/prettier": "^1.18.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "vue-jest": "^3.0.4"
  }
}
