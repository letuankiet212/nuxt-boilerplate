{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js Boilerplate",
  "author": "keitakn",
  "private": true,
  "scripts": {
    "dev": "rm -rf .nuxt && rm -rf dist && nuxt-ts",
    "build": "rm -rf .nuxt && rm -rf dist && nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "rm -rf .nuxt && rm -rf dist && nuxt-ts generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --collectCoverage=true",
    "test:e2e": "testcafe chrome test/e2e/pages/*.ts",
    "precommit": "npm run lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.3",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/bulma": "^1.2.4",
    "cookie-parser": "^1.4.4",
    "core-js": "2.6.9",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.9.1",
    "ts-node": "^8.3.0",
    "universal-cookie": "^4.0.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.1.11",
    "@nuxtjs/eslint-config-typescript": "^0.1.2",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/vue": "^5.1.11",
    "@types/cookie-parser": "^1.4.2",
    "@types/eslint": "^6.1.1",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/prettier": "^1.18.2",
    "@types/storybook__vue": "^5.0.2",
    "@types/universal-cookie": "^2.2.0",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.39.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "testcafe": "^1.4.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "vue-jest": "^3.0.4"
  }
}
